<script>
function playSong() {
  var audio = new Audio('https://raw.githubusercontent.com/jassie24/jaspreet_/main/joyful-snowman_60sec-176773.mp3');
  audio.play();
}

function playVideo() {
  var video = document.createElement('video');
  video.src = 'https://raw.githubusercontent.com/jassie24/jaspreet_/main/4173415-uhd_3840_2160_30fps.mp4';
  video.controls = true;
  video.autoplay = true;
  document.body.appendChild(video);
}
</script>

